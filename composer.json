{
  "name": "dynamic/foxystripe-mailchimp",
  "description": "FoxyStripe Mailchimp allows users to subscribe to a mailchimp mailing list when checking out.",
  "keywords": [
    "silverstripe",
    "foxycart",
    "foxystripe",
    "datafeed",
    "dynamic",
    "ecommerce",
    "mailchimp"
  ],
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dynamic Inc",
      "email": "dev@dynamicagency.com",
      "homepage": "http://www.dynamicagency.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "drewm/mailchimp-api": "^2.4",
    "dynamic/foxystripe": "^4.0",
    "sheadawson/silverstripe-dependentdropdownfield": "^2.0",
    "silverstripe/recipe-core": "^4.2",
    "silverstripe/vendor-plugin": "^1.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\FoxyStripeMailChimp\\": "src/",
      "Dynamic\\FoxyStripeMailChimp\\Test\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 600
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "expose": [
      "client"
    ]
  }
}
